<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tribune.dao.UserDao">
    <resultMap id="baseResultMap" type="com.tribune.pojo.User">
        <id column="id" property="id" jdbcType="java.lang.Integer"/>
        <result column="username" property="userName" jdbcType="java.lang.String"/>
        <result column="password" property="password" jdbcType="java.lang.String"/>
        <result column="company" property="company" jdbcType="java.lang.String"/>
        <result column="age" property="age" jdbcType="java.lang.Integer"/>
        <result column="sex" property="sex" jdbcType="java.lang.Integer"/>
    </resultMap>
    <sql id="cloumnList">
        username,passord,company,age,sex
    </sql>
    <select id="findById" resultType="com.tribune.pojo.User">
        select * from user by id;
    </select>

</mapper>